{
	"manifest_version": 1,

	"name": "Baby Boomers to Entitled Elderly",
    "short_name": "Entitled Elderly",
	"description": "Replaces the text 'Baby Booomer' with 'The Entitled Elderly'.",
	"author": "Caelyn McAulay",
	"version": "1.0",

	"icons": {
		"16": "entitled_16.png",
		"32": "entitled_32.png",
		"48": "entitled_48.png",
		"128": "entitled_128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}
